<table>
 	<tr>
 		<td>Name</td>
 		<td>Email</td>
 		<td>Password</td>
 	</tr>
 	<% @users.each do |user| %>
 	<tr>
 		<td><%= user.name %></td>
 		<td><%= user.email %></td>
 		<td><%= user.password %></td>
 		<td><%= link_to "destroy user", user, method: :delete %>
</td>
 		<td><%= link_to "show user", user_path(user.id) %></td>
 		<td><%= link_to "edit user", edit_user_path(user.id) %></td>
 	</tr>
 	<% end %>
</table>
